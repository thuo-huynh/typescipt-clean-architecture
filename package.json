{
  "name": "clean-architecture",
  "version": "1.0.0",
  "description": "This is project that implement follow clean architecture",
  "main": "/src/server.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && tsc --project tsconfig.build.json",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register src/server.ts",
    "prettier-format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "author": "thuohuynh",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "pg": "^8.9.0",
    "reflect-metadata": "^0.1.13",
    "translation-google": "^0.2.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typeorm": "^0.3.12",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
